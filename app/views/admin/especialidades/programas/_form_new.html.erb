<%= simple_form_for [:admin, @especialidad, @programa] do |f| %>
  <%= f.input :nombre, label: false %>
  <div class="d-flex flex-row justify-content-between">
    <%= f.button :submit, class: "btn btn-primary btn-block col-4 " %>
    <%= link_to "Volver", :back, class: "btn btn-danger btn-block col-4 m-0" %>
  </div>
<% end %>